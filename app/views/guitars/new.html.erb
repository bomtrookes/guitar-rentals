<div class="container">
  <h1>New Guitar</h1>
  <%= simple_form_for @guitar do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :caption %>
    <%= f.input :description %>
    <%= f.input :cost_per_day %>
    <%= f.input :guitar_type %>
    <%= f.submit %>
  <% end %>
</div>
<br>
